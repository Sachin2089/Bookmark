{
    "manifest_version": 3,
    "name": "Simple Bookmark Manager",
    "version": "1.0",
    "description": "A minimalistic Chrome extension for bookmark management",
    "permissions": ["storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
    
  }
  